<!DOCTYPE html>
<html>
<head>
  <title>LocalizationExample</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>
</head>
<style>
  .map-icon-label .map-icon {
  font-size: 24px;
  color: #FFFFFF;
  line-height: 48px;
  text-align: center;
  white-space: nowrap;
  }
</style>
<body>

  <div class="navbar navbar-default">
    <div class="navbar-header">
      <a href="#" class="navbar-brand">Localizations Tracking</a>
    </div>
    <div class="navbar-right">
      <p class="navbar-text">
        <%= link_to 'Historico', root_path, class:"navbar-link btn btn-default", method: :get %>
        <%= link_to 'Tiempo Real', vehicles_path, class:"navbar-link btn btn-default",method: :get %>
      </p>
    </div>
  </div>

<%= yield %>

</body>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
</html>
