<div class="col-md-4">
  <div class="well">
    <div class="page-header">
      <h3>Historico</h3>
    </div>
    <div class="form-group">
      <label for="vehicle">Select Vehicle</label>
      <select id="select-vehicle" class="form-control">
      </select>
    </div>
    <div class="form-group">
      <label for="date">Select Date</label>
      <input type="text" name="name" value="" id="txt-date" placeholder="Fecha" class="form-control">
    </div>
    <div class="row">
      <div class="col-md-6">
        <div class="form-group">
          <div class="input-group clockpicker ">
              <label for="">Start Time</label>
              <input type="text" class="form-control" value="" id="start_time" placeholder="Hora inicial">
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <div class="input-group clockpicker">
              <label for="">End Time</label>
              <input type="text" class="form-control" value="" id="end_time" placeholder="Hora Final">
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <div class="form-group">
          <button type="button" class="btn btn-primary" name="button" id="btn-search">Search</button>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA1Gg_rgvsutHRAJ2PHdKG85csmonHURjE"></script>
<div class="col-md-8">
  <div id="vehicles-map" style="width:100%; height:500px;">
  </div>
</div>
